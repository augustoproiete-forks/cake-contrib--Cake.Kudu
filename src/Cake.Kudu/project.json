{
  "version": "0.5.0-*",
  "description": "Cake Kudu AddIn",
  "copyright": "Copyright © WCOM AB 2017",
  "title": "Cake.Kudu",
  "authors": [
    "WCOM AB"
  ],
  "packOptions": {
    "summary": "Cake AddIn that extends Cake with Kudu evironment and deployment features",
    "projectUrl": "https://github.com/WCOMAB/Cake.Kudu/",
    "licenseUrl": "https://github.com/WCOMAB/Cake.Kudu/blob/master/LICENSE",
    "iconUrl": "https://cdn.rawgit.com/cake-contrib/graphics/a5cf0f881c390650144b2243ae551d5b9f836196/png/cake-contrib-medium.png",
    "releaseNotes": "Updated to Cake 0.22.0",
    "requireLicenseAcceptance": false,
    "tags": [
      "Cake",
      "Script",
      "Build",
      "Kudu",
      "Azure",
      "Deployment"
    ],
    "owners": [
      "devlead",
      "WCOMAB",
      "cake-contrib"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/WCOMAB/Cake.Kudu"
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "xmlDoc": true,
    "platform": "anycpu",
    "compile": {
      "includeFiles": [
        "../SolutionInfo.cs"
      ]
    }
  },
  "dependencies": {
    "Cake.Core": {
      "version": "0.22.0",
      "type": "build"
    },
    "Cake.Common": {
      "version": "0.22.0",
      "type": "build"
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.1",
        "System.Collections.NonGeneric": "4.3.0",
        "System.Collections.Specialized": "4.3.0"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Xml": {
          "type": "build"
        },
        "System.Net.Http": {
          "type": "build"
        }
      }
    }
  }
}